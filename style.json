{
  "style": {
    "theme": "dark",
    "colors": {
      "background_primary": "#0D1B2A",
      "background_secondary": "#11233B",
      "background_card": "#13293D",
      "text_primary": "#FFFFFF",
      "text_secondary": "#A3B9CC",
      "accent_primary": "#28E0B8",
      "accent_secondary": "#1BC29D",
      "border_light": "#1E3A5F"
    },
    "typography": {
      "font_family": "Inter, sans-serif",
      "headings": {
        "color": "#FFFFFF",
        "weight": 700,
        "size_main_title": "3rem",
        "size_subtitle": "1.2rem"
      },
      "body": {
        "color": "#A3B9CC",
        "weight": 400,
        "size": "1rem"
      }
    },
    "buttons": {
      "primary": {
        "background": "#28E0B8",
        "color": "#0D1B2A",
        "rounded": "999px",
        "padding": "12px 24px",
        "hover_opacity": 0.9
      },
      "secondary": {
        "background": "transparent",
        "border_color": "#28E0B8",
        "color": "#28E0B8",
        "rounded": "999px",
        "padding": "12px 24px"
      }
    },
    "inputs": {
      "background": "#11233B",
      "text_color": "#FFFFFF",
      "placeholder_color": "#7B8FA3",
      "rounded": "10px",
      "padding": "12px"
    },
    "layout": {
      "hero_section": {
        "alignment": "left",
        "padding": "80px",
        "max_width": "1200px",
        "image_position": "right",
        "content_vertical_alignment": "center",
        "content_width": "50%"
      },
      "grid_elements": {
        "dots_pattern_color": "#28E0B8",
        "dots_pattern_opacity": 0.25
      }
    },
    "components": {
      "logo_area": {
        "font_size": "1.4rem",
        "logo_color": "#28E0B8"
      },
      "hero_title": {
        "uppercase": true,
        "letter_spacing": "1px"
      },
      "feature_icons": {
        "color": "#A3B9CC",
        "size": "20px"
      }
    }
  }
}
